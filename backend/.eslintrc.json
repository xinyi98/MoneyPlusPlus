{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jasmine": true,
    "jest": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "consistent-return": "off",
    "operator-linebreak": "off",
    "object-curly-newline": "off"
  }
}
